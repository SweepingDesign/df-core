{
  "name": "dreamfactory/df-core",
  "description": "DreamFactory(tm) Core Components",
  "config": {
    "discard-changes": true
  },
  "homepage": "https://www.dreamfactory.com/",
  "keywords": [
    "dsp",
    "rest",
    "api",
    "mbaas",
    "baas",
    "paas"
  ],
  "license": ["Apache-2.0"],
  "authors": [
    {
      "name": "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    },
    {
      "name": "Lee Hicks",
      "email": "leehicks@dreamfactory.com"
    },
    {
      "name": "Jerry Ablan",
      "email": "jerryablan@dreamfactory.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~5.4",
    "dreamfactory/php-utils": "dev-develop as dev-master",
    "dreamfactory/df-sqldb-core": "dev-develop as dev-master",
    "guzzlehttp/guzzle": "~5.0",
    "barryvdh/laravel-cors": "0.7.x"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-4": {
      "DreamFactory\\Core\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.0.x-dev"
    }
  }
}
